/* Groups */
/begin GROUP params "Calibration Segment" ROOT /begin REF_CHARACTERISTIC params.reset params.t1_correction params.drift_correction params.drift_filter_size params.jitter_rms_filter_size params.jitter_avg_filter_size params.max_correction params.servo_i_gain params.servo_p_gain /end REF_CHARACTERISTIC /end GROUP
/begin GROUP PTP_SYNC "Measurement event group"  /begin REF_MEASUREMENT gPtpC.gm.domain gPtpC.gm.uuid gPtpC.gm.addr gPtpC.sync_local_time gPtpC.sync_master_time gPtpC.sync_correction gPtpC.sync_sequenceId gPtpC.sync_steps gPtpC.sync_cycle_time gPtpC.flup_master_time gPtpC.flup_sequenceId gPtpC.flup_correction gPtpC.t1_norm gPtpC.t2_norm gPtpC.master_drift_raw gPtpC.master_drift gPtpC.master_drift_drift gPtpC.master_offset_raw gPtpC.master_offset_compensation gPtpC.master_offset_detrended gPtpC.master_offset_detrended_filtered gPtpC.master_jitter gPtpC.master_jitter_rms gPtpC.master_jitter_avg gPtpC.servo_integral /end REF_MEASUREMENT /end GROUP
